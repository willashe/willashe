  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --env=dev --progress --profile",
    "build": "npm run cleanup && webpack --env=prod --progress --profile",
    "cleanup": "npm run prettier && npm run stylelint",
    "prettier": "prettier './src/{,!(node_modules)/**/}*.js' --single-quote --trailing-comma es5 --write",
    "stylelint": "stylelint './src/**/*.css' --fix",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --write",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {

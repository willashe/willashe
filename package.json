    "prettier": "^1.5.3",
    "react-hot-loader": "next",
    "style-loader": "^0.20.1",
    "stylelint": "^8.0.0",
    "stylelint-config-recommended": "^2.0.1",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^4.1.0"
  }
